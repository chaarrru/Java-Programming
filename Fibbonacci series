import java.util.*;
class fib{
    public static void main(String[] args){
        Scanner my = new Scanner(System.in);
        int a = my.nextInt();
        fibbo(a);
    }
    static void fibbo(int x){
        int a=-1,b=1,c,i;
        for(i=1;i<=x;i++){
            c=a+b;
            a=b;
            b=c;
        System.out.print(c+"  ");
        }
    }
}
